            <TextBox HorizontalAlignment="Left" Margin="133,13,0,0" x:Name="tb_sum" Width="80" IsReadOnly="True" Text="0" Height="25" VerticalAlignment="Top" />
            <TextBox Height="25" HorizontalAlignment="Left" Margin="297,13,0,0" x:Name="tb_aver" VerticalAlignment="Top" Width="80" IsReadOnly="True" Text="0" />
            <TextBox Height="25" Margin="568,13,7,0" x:Name="tb_result" VerticalAlignment="Top" IsReadOnly="True" Text="0" />
            <Label Content="×" FontSize="15" Height="31" HorizontalAlignment="Left" Margin="106,10,0,0" x:Name="label3_Copy1" VerticalAlignment="Top" Width="27" />
            <Label Content=" + " FontSize="15" Height="31" HorizontalAlignment="Left" Margin="213,10,0,0" x:Name="label3_Copy3" VerticalAlignment="Top" Width="27" />
            <Label Content=" + " FontSize="15" Height="31" HorizontalAlignment="Left" Margin="377,10,0,0" x:Name="label3_Copy5" VerticalAlignment="Top" Width="27" />
            <TextBox Height="25" HorizontalAlignment="Left" Margin="240,13,0,0" x:Name="tb_a2" VerticalAlignment="Top" Width="30" Text="0" PreviewTextInput="tb_a2_PreviewTextInput" />
            <TextBox Height="25" HorizontalAlignment="Left" Margin="404,13,0,0" x:Name="tb_a3" VerticalAlignment="Top" Width="30" Text="0" PreviewTextInput="tb_a3_PreviewTextInput"/>
            <Label Content="×" FontSize="15" Height="31" HorizontalAlignment="Left" Margin="270,10,0,0" x:Name="label3_Copy2" VerticalAlignment="Top" Width="27" />
            <Label Content="×" FontSize="15" Height="31" HorizontalAlignment="Left" Margin="434,10,0,0" x:Name="label3_Copy6" VerticalAlignment="Top" Width="27" />
            <Label Content=" = " FontSize="15" Height="31" HorizontalAlignment="Left" Margin="541,10,0,0" x:Name="label3_Copy4" VerticalAlignment="Top" Width="27" />
            <TextBox Height="25" HorizontalAlignment="Left" Margin="461,13,0,0" x:Name="tb_vari" VerticalAlignment="Top" Width="80" IsReadOnly="True" Text="0" />

        </Grid>
        <TextBox Height="28" HorizontalAlignment="Left" Margin="339,76,0,0" x:Name="tb_csv" VerticalAlignment="Top" Width="100" Visibility="Hidden" Text="4" />
        <TextBox x:Name="tb_detail" Margin="10,145,10,101" TextWrapping="Wrap" FontSize="13.333" IsReadOnly="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>

    </Grid>
</Window>
