        <TextBox Height="28" Margin="10,112,131,0" Name="textBox2" VerticalAlignment="Top" IsReadOnly="True" />
        <Button Content="Open..." Height="28" Margin="0,112,10,0" Name="btn_result" VerticalAlignment="Top" Click="btn_result_Click" HorizontalAlignment="Right" Width="95" />

        <Grid Height="50" Margin="0,0,0,46" VerticalAlignment="Bottom">
