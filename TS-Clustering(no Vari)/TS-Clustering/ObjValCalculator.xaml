        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TS_Clustering"
        mc:Ignorable="d"
        Title="ObjVal Calculator" Height="409.756" Width="688.85" ResizeMode="NoResize">
    <Grid>
        <TextBox Height="28" Margin="10,43,131,0" Name="textBox1" VerticalAlignment="Top" IsReadOnly="True" />
        <Button Content="Open..." Height="28" Margin="0,43,10,0" Name="btn_data" VerticalAlignment="Top" Click="btn_data_Click" HorizontalAlignment="Right" Width="95" />
